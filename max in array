package day1z;
import java.util.*;
public class Day1z {
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		int i;
		int n=s.nextInt();
		int[] a=new int[n];
		for(i=0;i<n;a[i++]=s.nextInt());
		int max=a[0];
		for(i=1;i<n;i++) {
			if(a[i]>max) {
				max=a[i];
			}
		}
		System.out.println(max);
		
	}
		
	}

